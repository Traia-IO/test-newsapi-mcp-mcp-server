{
  "github_url": "https://github.com/Traia-IO/test-newsapi-mcp-mcp-server",
  "mcp_server": {
    "name": "test-newsapi-mcp-mcp",
    "description": "Test deployment of newsapi - news aggregation from 80,000+ sources",
    "server_type": "streamable-http",
    "requires_api_key": true,
    "api_key_header": "Authorization",
    "api_keys": [
      "TEST_NEWSAPI_MCP_API_KEY"
    ],
    "capabilities": [
      "search_everything",
      "get_api_info"
    ],
    "payment_protocol": "402",
    "dual_mode": true,
    "dual_mode_description": "Supports both authenticated (free with API key) and payment (402 protocol) modes"
  },
  "deployment_method": "cloud_run",
  "gcp_project_id": "traia-mcp-servers",
  "gcp_region": "us-central1",
  "tags": [
    "test-newsapi-mcp",
    "api",
    "mcp"
  ],
  "ref": "main"
}